	ADD	ATOM HC61 TYPE=H    CHARge= 0.100   END
	ADD	ATOM HC62 TYPE=H    CHARge= 0.100   END
	ADD	ATOM HC63 TYPE=H    CHARge= 0.100   END
	MODIfy	ATOM C6             CHARge=-0.100   END

	! Bonds
	ADD	BOND C6   N6
	ADD	BOND N6   HN6
	ADD	BOND N6   CN6
	ADD	BOND CN6  OCN6
	ADD	BOND CN6  CCN6
	ADD	BOND CCN6 HC61
	ADD	BOND CCN6 HC62
	ADD	BOND CCN6 HC63

	! Angles
	ADD	ANGLe C5   C6   N6
	ADD	ANGLe H61  C6   N6
	ADD	ANGLe H62  C6   N6
	ADD	ANGLe C6   N6   HN6
	ADD	ANGLe C6   N6   CN6
	ADD	ANGLe N6   CN6  OCN6
	ADD	ANGLe N6   CN6  CCN6
	ADD     ANGLe CCN6 CN6  OCN6  
	ADD	ANGLe CN6  CCN6 HC61
	ADD	ANGLe CN6  CCN6 HC62
	ADD	ANGLe CN6  CCN6 HC63
	ADD	ANGLe HC61 CCN6 HC62
	ADD	ANGLe HC61 CCN6 HC63
	ADD	ANGLe HC62 CCN6 HC63

	! Improper angles for chirality of substituants
	ADD	IMPRoper H61  H62  C5   N6

	! Improper angles for maintenance of the N-Acetyl group
	ADD	IMPRoper N6   C6   HN6  CN6	! N planar in N-Acetyl group
	ADD	IMPRoper CN6  N6   CCN6 OCN6	! C planar in N-Acetyl group
	ADD	IMPRoper OCN6 CN6  N6   HN6	! N-H and C=O trans in N-Acetyl group
END

PRESidue C6Ca		! The C6(OH)H is replaced by a Carboxylique group
	DELEte  ATOM O6                             END
	DELEte  ATOM HO6                            END
	DELEte  ATOM H61                            END
	DELEte  ATOM H62                            END
	ADD     ATOM O61  TYPE=OC   CHARge=-0.800   END
	ADD     ATOM O62  TYPE=OC   CHARge=-0.800   END
	MODIfy  ATOM C6   TYPE=C    CHARge= 0.700   END

	! Bonds
	ADD     BOND C6   O61
	ADD     BOND C6   O62

	! Angles
	ADD     ANGLe C5   C6   O61
	ADD     ANGLe C5   C6   O62

	! Improper angle for planar C6 of the carboxyl group
	IMPRoper C6  C5   O62  O61	! Carboxylic group
END

